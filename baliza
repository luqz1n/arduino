int led1 = 2;
int led2 = 16;
void setup() {

pinMode(led1, OUTPUT);
pinMode(led2, OUTPUT);
}

// la baliza se enciende cada 500 milisegundos
void loop() {
  digitalWrite(led1, HIGH);
  delay(100);
  digitalWrite(led1, LOW);
  delay(50);
  digitalWrite(led2, HIGH);
  delay(100);
  digitalWrite(led2, LOW);                       
  delay(500);                      
}
